{
    "name": "w4/laravelkit",
    "description": "Paquete de Automatizacion Inicial de Configuracion de Proyectos LaravelLivewire PHP.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "w4laravelkit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Francisco Morales",
            "email": "franciscomorales@w4.software.com",
            "role": "Desarrollador"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "W4LaravelKit\\Providers\\W4LaravelKitServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/console": "*"
    },
    "require-dev": {
        "laravel/framework": "^v12.0",
        "laravel/pint": "^1.16",
        "illuminate/support": "*"
    },
    "conflict": {
        "laravel/framework": "<12.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
